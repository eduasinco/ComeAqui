<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:layout_width="match_parent"
                                             android:layout_height="match_parent" android:id="@+id/listItem">

    <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="260dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent">

        <TextView
                android:text="Comida - Localizacion "
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:id="@+id/foodName" app:layout_constraintStart_toStartOf="@+id/guideline6"
                app:layout_constraintTop_toTopOf="@+id/guideline5"
                app:layout_constraintBottom_toTopOf="@+id/guideline3" android:width="200dp"
                android:fontFamily="sans-serif-condensed" android:textAlignment="viewStart"
                android:textColor="@android:color/background_dark" android:visibility="visible"
                android:gravity="center"/>
        <ImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:srcCompat="@drawable/hamburger"
                android:id="@+id/foodImage"
                android:background="#257701ff"
                app:layout_constraintBottom_toTopOf="@+id/guideline5"
                app:layout_constraintEnd_toStartOf="@+id/guideline7"
                app:layout_constraintStart_toStartOf="@+id/guideline6"
                app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="16dp"/>
        <TextView
                android:text="Precio - 30$"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:id="@+id/price" app:layout_constraintStart_toStartOf="@+id/guideline6"
                app:layout_constraintTop_toTopOf="@+id/guideline3"
                android:width="200dp"
                android:fontFamily="sans-serif-condensed" app:layout_constraintBottom_toBottomOf="parent"
                android:gravity="center" android:textAlignment="viewStart"/>
        <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                              android:id="@+id/guideline6"
                                              android:orientation="vertical"
                                              app:layout_constraintGuide_begin="35dp"/>
        <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                              android:id="@+id/guideline3" app:layout_constraintGuide_begin="230dp"
                                              android:orientation="horizontal"/>
        <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                              android:id="@+id/guideline7"
                                              android:orientation="vertical"
                                              app:layout_constraintGuide_end="35dp"/>
        <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                              android:id="@+id/guideline5" app:layout_constraintGuide_begin="200dp"
                                              android:orientation="horizontal"/>
        <android.support.constraint.ConstraintLayout
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintEnd_toStartOf="@+id/guideline7"
                app:layout_constraintTop_toTopOf="@+id/guideline5"
                app:layout_constraintStart_toEndOf="@+id/foodName"
                app:layout_constraintBottom_toTopOf="@+id/guideline3">

            <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="0dp" app:srcCompat="@drawable/vegetarianfill"
                    android:id="@+id/imageView2"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/imageView4"/>
            <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="0dp" app:srcCompat="@drawable/vegetarianfill"
                    android:id="@+id/imageView3"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintVertical_bias="0.0"
                    app:layout_constraintEnd_toStartOf="@+id/imageView6"/>
            <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="0dp" app:srcCompat="@drawable/vegetarianfill"
                    android:id="@+id/imageView4"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintVertical_bias="0.0"
                    app:layout_constraintEnd_toStartOf="@+id/imageView3"/>
            <ImageView
                    android:layout_width="23dp"
                    android:layout_height="30dp" app:srcCompat="@drawable/vegetarianfill"
                    android:id="@+id/imageView5"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintVertical_bias="0.0"
                    app:layout_constraintEnd_toEndOf="parent"/>
            <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="0dp" app:srcCompat="@drawable/vegetarianfill"
                    android:id="@+id/imageView7"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintVertical_bias="0.0"
                    app:layout_constraintEnd_toStartOf="@+id/imageView5"/>
            <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="0dp" app:srcCompat="@drawable/vegetarianfill"
                    android:id="@+id/imageView6"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintVertical_bias="0.0"
                    app:layout_constraintEnd_toStartOf="@+id/imageView7"/>
        </android.support.constraint.ConstraintLayout>
    </android.support.constraint.ConstraintLayout>

</android.support.constraint.ConstraintLayout>